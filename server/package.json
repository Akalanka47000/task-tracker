{
  "name": "server",
  "version": "0.0.1",
  "author": "Akalanka Perera",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "start": "nest start --builder swc",
    "start:prod": "node --enable-source-maps dist/main.js",
    "dev": "nest start --watch --debug",
    "lint": "sh ../scripts/lint.sh",
    "test": "jest --coverage --forceExit --runInBand",
    "typeorm": "TYPEORM_CLI=true ts-node -T -P ./tsconfig.json -r tsconfig-paths/register ../node_modules/typeorm/cli.js --dataSource ./src/database/postgres/index.ts",
    "migration:generate": "sh ./scripts/migration-generate.sh",
    "migration:create": "sh ./scripts/migration-create.sh",
    "migrate": "npm run typeorm migration:run",
    "rollback": "npm run typeorm migration:revert",
    "seed": "ts-node -T -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs --dataSource ./src/database/postgres/index.ts seed:run"
  },
  "dependencies": {
    "@nestjs/common": "11.0.1",
    "@nestjs/core": "11.0.1",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/swagger": "11.0.0",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/typeorm": "11.0.0",
    "@shared/constants": "workspace:*",
    "@sliit-foss/functions": "2.10.0",
    "@sliit-foss/http-logger": "1.4.0",
    "@sliit-foss/module-logger": "1.3.1",
    "@sliit-foss/mongoose-aggregate-paginate-v2": "3.0.0",
    "@sliit-foss/mongoose-audit": "1.0.2",
    "@sliit-foss/mongoose-filter-query": "7.0.2",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "class-validator-jsonschema": "5.0.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.7",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "express-rate-limit": "7.2.0",
    "firebase-admin": "13.2.0",
    "helmet": "7.1.0",
    "ioredis": "5.2.3",
    "jsonwebtoken": "9.0.0",
    "pg": "8.13.3",
    "qs": "6.14.0",
    "rate-limit-redis": "4.2.0",
    "redlock": "4.2.0",
    "reflect-metadata": "0.2.2",
    "request-ip": "3.3.0",
    "rxjs": "7.8.1",
    "typeorm": "0.3.20",
    "typeorm-extension": "3.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.0",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@sliit-foss/actions-exec-wrapper": "1.1.2",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.13",
    "@types/apicache": "1.6.6",
    "@types/bcryptjs": "2.4.6",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.8",
    "@types/cors": "2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/redlock": "4.0.7",
    "@types/request-ip": "0.0.41",
    "@types/supertest": "^6.0.2",
    "jest": "29.7.0",
    "supertest": "7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.2"
  }
}